<h1>Listing job_postings</h1>

<table>
  <thead>
    <tr>
      <th>Job id</th>
      <th>Keyword</th>
      <th>City</th>
      <th>Province</th>
      <th>Country</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_postings.each do |job_posting| %>
      <tr>
        <td><%= job_posting.job_ID %></td>
        <td><%= job_posting.keyword %></td>
        <td><%= job_posting.city %></td>
        <td><%= job_posting.province %></td>
        <td><%= job_posting.country %></td>
        <td><%= job_posting.company %></td>
        <td><%= link_to 'Show', job_posting %></td>
        <td><%= link_to 'Edit', edit_job_posting_path(job_posting) %></td>
        <td><%= link_to 'Destroy', job_posting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job posting', new_job_posting_path %>
